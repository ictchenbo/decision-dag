速度[X, Y] = 最大速度[Y] * (1 - 环境影响因子[X])
环境影响因子[X] = 0.5 * 天气影响因子[X] + 0.5 * 地形影响因子[X]
天气影响因子[X] = 风速影响因子[X] + 雨影响因子[X] + 温度影响因子[X]
风速影响因子[X] = 风速[X] / 20.0
雨影响因子[X] = 降雨量[X] / 100
温度影响因子[X] = 0
(温度影响因子[X] == 0.1) <= (温度[X] <= -10)
(温度影响因子[X] == 0.1) <= (温度[X] >= 40) & (温度[X] < 60)
(温度影响因子[X] == 0.2) <= (温度[X] > 60)
(温度影响因子[X] == 0.2) <= (温度[X] < -10)
地形影响因子[X] = 地形因子[地形[X]]
地形因子['平原'] = 0
地形因子['高原'] = 0.3
地形因子['山地'] = 0.5
地形因子['沼泽'] = 0.7
地形因子['水体'] = 0.9

最大速度['人员1'] = 10
